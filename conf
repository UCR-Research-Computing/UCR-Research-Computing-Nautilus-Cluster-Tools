[pipeline-series]
step2 = cp /sharedvol/ursa-data-connect-input/zebrafish.top.faa .;
        cp /sharedvol/ursa-data-connect-input/mouse.1.protein.faa .;
        cp /sharedvol/ursa-data-connect-input/zebrafish.1.protein.faa .;
        ./makeblastdb -in mouse.1.protein.faa -dbtype prot; ./makeblastdb -in zebrafish.1.protein.faa -dbtype prot;
        ./blastp -query zebrafish.1.protein.faa -db mouse.1.protein.faa -num_threads 4 -out zebrafish.x.mouse;tar -zcvf job-output.tar.gz zebrafish.x.mouse;
        cp job-output.tar.gz /sharedvol/zzz$(hostname)-job-output.tar.gz
